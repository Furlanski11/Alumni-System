@model AlumniSystem.Core.ViewModels.CommunityViewModel

<h1>Изтриване на общност</h1>
<h3>Сигурни ли сте, че искате да изтриете тази общност?</h3>
<dl class="row">
	<dt class="col-sm-2">Име</dt>
	<dd class="col-sm-10">@Model.Name</dd>
	<dt class="col-sm-2">Описание</dt>
	<dd class="col-sm-10">@Model.Description</dd>
</dl>
<form asp-action="Delete" method="post">
	<input type="hidden" asp-for="Id" />
	<button type="submit" class="btn btn-danger mt-2">Да, изтрий</button>
	<a asp-action="Index" class="btn btn-secondary mt-2">Не, върни ме назад</a>
</form>